{
  "homepage": "http://anthonymartin36.github.io/cocktail-recipes",
  "name": "cocktail-recipes",
  "type": "module",
  "version": "1.0.0",
  "description": "Boilerplate - cocktail-recipes",
  "main": "server/index.ts",
  "engines": {
    "node": "20.13.1"
  },
  "scripts": {
    "test": "vitest",
    "start": "node server/index.ts",
    "dev": "run-p dev:client dev:server",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "run-s build:client build:server",
    "build:client": "vite build",
    "build:server": "esbuild --packages=external --platform=node --format=esm --outfile=dist/server.js --bundle server/index.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonymartin36/cocktail.git"
  },
  "keywords": [
    "AMartin",
    "boilerplate"
  ],
  "author": "AMartin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anthonymartin36/cocktail"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node": "^18.20.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1",
    "superagent": "^9.0.2",
    "tsx": "^4.11.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.37.1",
    "@tanstack/react-query-devtools": "^5.37.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/superagent": "^8.1.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
